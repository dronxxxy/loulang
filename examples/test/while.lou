const putChar = @externFun("putchar", @procFun(u8))

const putInt = fun (var int: i32) {
  if int < 0 {
    # TODO: unary operators
    int = 0 - int
    putChar('-')
  }
  if int == 0 {
    putChar('0')
  }
  while int > 0 {
    putChar('0' + @intCast(int % 10, u8))
    int = int / 10
  }
}

@global("main", fun () -> i32 {
  # TODO: unary operators
  var i = 0 - 10
  
  while i <= 10 {
    putInt(i)
    putChar('\n')
    i = i + 1
  }

  return 0
})


